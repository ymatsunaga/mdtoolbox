#!/bin/sh

for i in $(seq --format=%02g 0 7)
do

ptraj ala.top << EOF
trajin rint${i}x.trj

dihedral d1 @7 @9 @15 @17 out rint${i}x.psi
dihedral d2 @5 @7 @9 @15  out rint${i}x.phi

EOF

ptraj ala.top << EOF
trajin rint${i}y.trj

dihedral d1 @7 @9 @15 @17 out rint${i}y.psi
dihedral d2 @5 @7 @9 @15  out rint${i}y.phi

EOF

done

